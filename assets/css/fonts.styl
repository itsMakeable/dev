// MAKEABLE ICONS
@font-face {
	font-family: "mkbl-icons";
	src:url("../font/mkbl-icons.eot");
	src: url("../font/mkbl-icons.eot?#iefix") format("embedded-opentype"),
		url("../font/mkbl-icons.woff") format("woff"),
		url("../font/mkbl-icons.ttf") format("truetype"),
		url("../font/mkbl-icons.svg#mkbl-icons") format("svg");
	font-weight: normal;
	font-style: normal;
}

[data-icon]:before {
  font-family: "mkbl-icons" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.mkbl-icon,
.mkbl-icons,
.mkbl-share-icon,
.mkbl-share-icons,
[class^="icon-"]:before,
[class*=" icon-"]:before {
  font-family: "mkbl-icons" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

